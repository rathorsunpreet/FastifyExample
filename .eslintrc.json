{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "linebreak-style": "off",
      "func-names": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "import/extensions": [
        "error",
        "always"
      ]
  }
}
